<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.tomst.lolly.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_graph"
        android:name="com.tomst.lolly.ui.graph.GraphFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_graph" />

    <fragment
        android:id="@+id/navigation_options"
        android:name="com.tomst.lolly.ui.options.OptionsFragment"
        android:label="Options"
        tools:layout="@layout/fragment_options" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.tomst.lolly.ui.viewfile.ListFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_viewer" />
</navigation>